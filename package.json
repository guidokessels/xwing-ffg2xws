{
  "name": "xwing-ffg2xws",
  "version": "0.0.0-development",
  "description": "Convert an X-Wing squad from FFG's squadbuilder to XWS.",
  "main": "dist/index.js",
  "author": "Guido Kessels <mail@guidokessels.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "test": "jest tests",
    "test:watch": "jest --watch",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,ts}": [
      "prettier --write",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "es5",
    "tabWidth": 2,
    "singleQuote": true
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-typescript": "^7.3.3",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "semantic-release": "^15.13.12",
    "ts-loader": "^6.0.0",
    "typescript": "^3.4.5",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guidokessels/xwing-ffg2xws.git"
  }
}
